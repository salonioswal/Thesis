# Loading work.shifter(behavioral_shifter)#1
add wave -position insertpoint  \
sim:/shifter/clock \
sim:/shifter/input_data \
sim:/shifter/output_data \
sim:/shifter/rd_ptr \
sim:/shifter/RF_data \
sim:/shifter/rst_n \
sim:/shifter/wr_en \
sim:/shifter/wr_ptr
force -freeze sim:/shifter/clock 1 0, 0 {5 ns} -r 10
force -freeze sim:/shifter/input_data 16'h0001 0
force -freeze sim:/shifter/wr_ptr 1 0, 0 {50 ns} -r 100
force -freeze sim:/shifter/wr_ptr 2'h01 0 -cancel 5
force -freeze sim:/shifter/wr_en 1 0 -cancel 10
force -freeze sim:/shifter/rd_ptr 2'h01 0
run
# ** Warning: NUMERIC_STD.TO_INTEGER: metavalue detected, returning 0
#    Time: 10 ns  Iteration: 0  Instance: /shifter
force -freeze sim:/shifter/rst_n 1 0
run
run
run
force -freeze sim:/shifter/wr_en 1 0
run
# Break key hit
quit -sim
